<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DetailTECMovilApp.Home"
            xmlns:local="clr-namespace:DetailTECMovilApp"
            Title ="Menu"
            BackgroundColor="White">

    <FlyoutPage.Flyout>

        <local:FlyoutMenuPage x:Name="flyout"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage BarBackgroundColor="#FFA534">
            <x:Arguments>
                <local:MainPage/>
            </x:Arguments>
        </NavigationPage>

    </FlyoutPage.Detail>
    
</FlyoutPage>